# GNU Make solution makefile autogenerated by Premake
# Type "make help" for usage help

ifndef config
  config=debug
endif
export config

PROJECTS := CwIRC MBFunctionInterface MBUtilities MoronBot

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

CwIRC: 
	@echo "==== Building CwIRC ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f CwIRC.make

MBFunctionInterface: CwIRC
	@echo "==== Building MBFunctionInterface ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f MBFunctionInterface.make

MBUtilities: CwIRC MBFunctionInterface
	@echo "==== Building MBUtilities ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f MBUtilities.make

MoronBot: CwIRC MBFunctionInterface MBUtilities
	@echo "==== Building MoronBot ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f MoronBot.make

clean:
	@${MAKE} --no-print-directory -C . -f CwIRC.make clean
	@${MAKE} --no-print-directory -C . -f MBFunctionInterface.make clean
	@${MAKE} --no-print-directory -C . -f MBUtilities.make clean
	@${MAKE} --no-print-directory -C . -f MoronBot.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   CwIRC"
	@echo "   MBFunctionInterface"
	@echo "   MBUtilities"
	@echo "   MoronBot"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
